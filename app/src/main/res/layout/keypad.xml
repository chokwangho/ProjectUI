<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="10"
        android:id="@+id/edit1"
        android:layout_gravity="center_horizontal"
        android:hint="수식입력"
        />

    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <TableRow>
            <Button
                android:id="@+id/btnIntegral"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="∫"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="∬"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="d/dx"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="lim"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="∞"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="e"
                android:textColor="#FFFFFF"/>
        </TableRow>

        <TableRow>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="sin"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="cos"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="tan"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="cot"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="sec"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="csc"
                android:textColor="#FFFFFF"/>
        </TableRow>
        <TableRow>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="x^2"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="y^x"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="√x"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="x√y"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="log"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="ln"
                android:textColor="#FFFFFF"/>
        </TableRow>

        <TableRow>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="x"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="y"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="z"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="π"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="θ"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="abc"
                android:textColor="#FFFFFF"/>
        </TableRow>

        <TableRow>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="1"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="2"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="3"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="4"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="5"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="6"
                android:textColor="#FFFFFF"/>
        </TableRow>

        <TableRow>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="7"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="8"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="9"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="0"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="("
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="°"
                android:textColor="#FFFFFF"/>
        </TableRow>

        <TableRow>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="+"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="-"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="*"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="/"
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="="
                android:textColor="#FFFFFF"/>
            <Button
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:background="#00D8FF"
                android:text="cel"
                android:textColor="#FFFFFF"/>
        </TableRow>

    </TableLayout>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">
        <Button
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:background="#00D8FF"
            android:text="GRA"
            android:textColor="#FFFFFF"/>
        <Button
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:background="#00D8FF"
            android:text="<"
            android:textColor="#FFFFFF"/>
        <Button
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:background="#00D8FF"
            android:text=">"
            android:textColor="#FFFFFF"/>
        <Button
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:background="#00D8FF"
            android:text="←"
            android:textColor="#FFFFFF"/>
        <Button
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:background="#00D8FF"
            android:text="→"
            android:textColor="#FFFFFF"/>
        <Button
            android:layout_width="58dp"
            android:layout_height="wrap_content"
            android:background="#00D8FF"
            android:text="SOL"
            android:textColor="#FFFFFF"/>
    </TableRow>
</LinearLayout>